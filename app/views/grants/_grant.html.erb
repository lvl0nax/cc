<li class="temp_grant templ" data-type="grant" data-content="<%= grant.id%>" onclick="showgrant('<%=grant.id%>');">
	<div class="temp_date"> <%= l grant.start_date, :format => :short %></div>
	<div class="temp_type">
		грант
	</div>
	<div class="temp_img">
		<% if grant.photo.blank? %>
      <%= image_tag "/assets/Untitled3.jpg" %>
    <% else %>
      <%= image_tag grant.photo.url %>
    <% end %>
	</div>
	<div class="temp_place">
		
	</div>
	<div class="temp_name">
		<%= grant.title %>
	</div>
</li>